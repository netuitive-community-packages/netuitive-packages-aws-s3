{
  "analyticConfiguration": {
    "metrics": [
      {
        "match": "aws.s3.bucketsizebytes",
        "properties": {
          "tags": {
            "unit": "bytes"
          }
        }
      },
      {
        "match": "aws.s3.numberofobjects",
        "properties": {
          "tags": {
            "unit": "count"
          }
        }
      }
    ],
    "name": "AWS S3",
    "scope": {
      "elementType": "S3"
    },
    "type": "METRIC_META"
  }
}